    },
    "clinical_approval_status": {
      "attribute_type_id": "biolink:clinical_approval_status"
    },
    "nctid": {
      "attribute_type_id": "biolink:supporting_study",
      "value_url": "https://clinicaltrials.gov/study/{****}?tab=table"
    },
    "phase": {
      "attribute_type_id": "biolink:clinical_trial_phase"
    },
    "overall_status": {
      "attribute_type_id": "biolink:clinical_trial_status"
    },
    "enrollment": {
      "attribute_type_id": "biolink:clinical_trial_study_size"
